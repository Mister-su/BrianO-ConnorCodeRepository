<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.dinfo.sfexp.db.BaseDb">  

  <select id="getConceptByType" resultType="java.util.ArrayList">
  		select  CONCAT(tree_id,'_',concept_id) cate ,resource  cate_name from concept_resource where type=#{type}
  </select>
</mapper> 